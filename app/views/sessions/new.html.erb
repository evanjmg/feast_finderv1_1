<header>
<h1> Sign in</h1>
</header>
<%= form_tag(session_path) do %>
<fieldset>
	<p>
		<%= label_tag "Email" %>
		<%= email_field_tag :email, nil, autofocus:true, required: true %>
	</p>
	<p>
		<%= label_tag :password %>
		<%= password_field_tag :password, nil, required: true %>
	</p><p>
	<%= submit_tag "Sign in" %>
	</p>
</fieldset>
	<% end %>
	<p>
  No account yet? <%= link_to 'Sign up!', signup_path %>
</p>